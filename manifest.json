{
  "name": "Chrome-Vim",
  "version": "0.1",
  "description": "Vim style keybindings for chrome.",
  "background_page": "vim_bg.html",
  "toolstrips": ["vim_strip.html"],
  "permissions": ["tabs"],
  "content_scripts": [
  {
          "matches": ["http://*/*", "https://*/*", "ftp://*/*"],
          "js": ["jquery-1.3.2.min.js", "jquery.hotkeys.js", "vim.js"],
          "css": ["vim.css"],
          "run_at": "document_start"
  }
  ]
}
