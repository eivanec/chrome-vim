{
  "name": "Chrome-Vim",
  "version": "0.1",
  "description": "Vim style keybindings for chrome.",
  "permissions": [
          "tabs"
          ],
  "content_scripts": [
  {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["jquery-1.3.2.min.js", "jquery.hotkeys.js", "vim.js"],
          "run_at": "document_start"
  }
  ]
}
